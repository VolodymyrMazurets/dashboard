    "@testing-library/user-event": "^7.1.2",
    "@types/antd": "^1.0.0",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.164",
